# ServiceLine API Configuration
NEXT_PUBLIC_SERVICELINE_API_URL=https://api.serviceline.com/v1
NEXT_PUBLIC_SERVICELINE_API_KEY=your_serviceline_api_key_here

# Database Configuration (Optional - if using database)
DATABASE_URL=postgresql://user:password@localhost:5432/serviceline_db

# Authentication (Optional - if implementing auth)
NEXTAUTH_URL=http://localhost:3000
NEXTAUTH_SECRET=your_nextauth_secret_here

# Email Service Configuration (Optional)
EMAIL_SERVICE_API_KEY=your_email_service_key
EMAIL_FROM=noreply@serviceline.com

# Analytics (Optional)
NEXT_PUBLIC_GA_MEASUREMENT_ID=G-XXXXXXXXXX

# Application Settings
NEXT_PUBLIC_APP_NAME=ServiceLine Dashboard
NEXT_PUBLIC_APP_VERSION=1.0.0
NODE_ENV=development

# Rate Limiting
API_RATE_LIMIT_PER_MINUTE=60

# File Upload Settings
MAX_FILE_SIZE_MB=10
ALLOWED_FILE_TYPES=csv,xlsx,json

# Feature Flags
ENABLE_LEAD_SCORING=true
ENABLE_EMAIL_CAMPAIGNS=true
ENABLE_ANALYTICS=true
ENABLE_EXPORT=true

# Scraper Configuration
SCRAPER_ENABLED=false
SCRAPER_FREQUENCY=daily
SCRAPER_MAX_LEADS_PER_RUN=1000

# Debug Settings
DEBUG_MODE=false
ENABLE_API_LOGGING=false
